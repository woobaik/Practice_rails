<!DOCTYPE html>
<html>
   <%= render 'layouts/head' %>
  <body>
    <%= render 'layouts/header' %>
    <div class="container">
      <% flash.each do |msg_type, msg| %>
        <div class="alert alert-<%= msg_type %>">
          <%= msg %>
        </div>
        <% end %>
      <%= yield %>
    <%= render 'layouts/footer'%>
    <%= debug(params) if Rails.env.development? %>
    </div>
  </body>
</html>
