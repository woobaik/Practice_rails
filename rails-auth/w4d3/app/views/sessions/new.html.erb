<h1>LOG IN!!!!!</h1>

<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username, placeholder: "Enter username" %><br>

  <%= f.label :password %>
  <%= f.password_field :password_digest, placeholder: "Enter Password" %><br>

  <%= f.submit "Log in Button" %>
<% end %>
